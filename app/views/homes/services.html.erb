  <script>
    $(document).ready(function() {
      $('.brand .owl-carousel').owlCarousel({
        loop: true,
        margin: 20,
        responsiveClass: true,
        nav:false,
        dots:true,
        responsive: {
          0: {
            items: 1,
            margin:0,
          },
          600: {
            items: 3,
          },
          1000: {
